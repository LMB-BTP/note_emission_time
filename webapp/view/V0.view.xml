<mvc:View controllerName="LMBR_CUSTOMER_APP.note_emission_time.controller.V0" displayBlock="true" xmlns:layout="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:table="sap.ui.table">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<Panel expandable="true" expanded="true" busy="{displayData>/Settings/panelBusy}">
							<headerToolbar>
								<OverflowToolbar height="3rem" style="Clear">
									<Label text="{i18n>panelHeaderText}" />
									<ToolbarSpacer/>
									<Button icon="sap-icon://download-from-cloud" tooltip="{i18n>buttDownloadTooTip}" press="_onPressDownload"/>
								</OverflowToolbar>
							</headerToolbar>
							<HBox id="TableVBox"/>
						</Panel>
						<VBox id="ChartVBox"/>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>